{
  "name": "reporting-api",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "copy-assets": "ts-node tools/copyAssets",
    "codegen": "graphql-codegen",
    "exec-deploy-staging": "serverless deploy --stage Staging",
    "deploy-staging": "npm-run-all build exec-deploy-staging",
    "exec-deploy-prod": "serverless deploy --stage Prod",
    "deploy-prod": "npm-run-all build exec-deploy-prod",
    "initdb": "ts-node tools/initdb",
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "tsc": "tsc",
    "build": "npm-run-all clean lint prisma:membership:gen prisma:attendance:gen tsc",
    "dev:start": "npm-run-all build start",
    "dev": "nodemon --watch src -e ts,ejs --exec npm run dev:start",
    "start": "node .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prisma:membership:gen": "prisma generate --schema ./prisma/membership/schema.prisma",
    "prisma:membership:introspect": "prisma introspect --schema ./prisma/membership/schema.prisma",
    "prisma:attendance:gen": "prisma generate --schema ./prisma/attendance/schema.prisma",
    "prisma:attendance:introspect": "prisma introspect --schema ./prisma/attendance/schema.prisma",
    "prisma:giving:gen": "prisma generate --schema ./prisma/giving/schema.prisma",
    "prisma:giving:introspect": "prisma introspect --schema ./prisma/giving/schema.prisma",
    "dev:watch": "npm run prisma:membership:gen && npm run prisma:attendance:gen && npm run prisma:giving:gen && tsc-watch --onSuccess \"node dist/index.js\"",
    "serverless-local": "serverless invoke local -f api"
  },
  "keywords": [],
  "author": "Live Church Solutions",
  "dependencies": {
    "@prisma/client": "^3.7.0",
    "@types/lodash": "^4.14.178",
